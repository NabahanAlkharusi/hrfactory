                    var ParentCard = "";
                    var Parentcollapse = "";
                    var ChildCard = "";
                    var Childcollapse = "";
                    var textToRight = System.Threading.Thread.CurrentThread.CurrentCulture.TextInfo.IsRightToLeft ? "text-right" : "";
                    <div class="col-lg-9" id="defult" dir="@dir">
                        <div class="card @textToRight" id="DefaultMBCard" style="display:none">
                            <div class="card-header">
                                <h5 class="mb-0 d-inline">
                                    @Resources.General.DefaultMB
                                </h5>

                            </div>
                            <div class="card-body">
                                <div class="alert alert-warning text-center"><h6>@Resources.General.UserManulNotif</h6></div>
                                <div id="accordion">

                                    @foreach (HumanResourceHelth.Model.Section section in Model.Sections.Where(x => x.ParenId == null))
                                    {
                                        ParentCard = "Parent" + parent.ToString();
                                        Parentcollapse = "collapse" + parent.ToString();
                                        parent = parent + 1;
                                        <div>
                                            <div class="card">
                                                <div class="card-header" id="@ParentCard">
                                                    <h5 class="mb-0 d-inline">
                                                        <button class="btn btn-link" data-toggle="collapse" data-target="#@Parentcollapse" aria-expanded="false" aria-controls="@Parentcollapse">
                                                            @Html.Raw(section.Title)
                                                        </button>
                                                    </h5>

                                                </div>
                                                <div id="@Parentcollapse" class="collapse" aria-labelledby="@ParentCard" data-parent="#accordion">

                                                    <div class="card-body">
                                                        @if (section.Childs.Count == 0)
                                                        {

                                                            <p>@Html.Raw(section.Content)</p>

                                                        }
                                                        else
                                                        {

                                                            foreach (HumanResourceHelth.Model.Section childSection in section.Childs)
                                                            {
                                                                ChildCard = "ChildCard" + Child.ToString();
                                                                Childcollapse = "Childcollapse" + Child.ToString();
                                                                Child = Child + 1;
                                                                <div class="card" id="@ChildCard">
                                                                    <div class="card-header">
                                                                        <a href="#" data-toggle="collapse" data-target="#@Childcollapse">@childSection.Title</a>
                                                                    </div>
                                                                    <div class="card-body collapse" data-parent="#@ChildCard" id="@Childcollapse">
                                                                        @Html.Raw(childSection.Content)
                                                                    </div>
                                                                </div>
                                                            }
                                                        }
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                }
