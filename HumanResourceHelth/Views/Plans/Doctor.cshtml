<section class="padding-tb-100px">
    <div class="container">
        <div class="section-title section-title-center">
            <h1 class="title cms" data-contentId="84">@GetContent(84) </h1>
            @*<span class="section-title-des cms" data-contentId="85">@GetContent(85) </span>*@
        </div>
        <div class="row justify-content-md-center">
            <div class="col-lg-8 text-uppercase">
                <form id="frmRequest" method="post" enctype="multipart/form-data">

                    <div class="form-group">
                        <div class="row">

                            <div class="col" style="font-family:Tahoma;">
                                <label>@Resources.General.Email</label>
                                <input autocomplete="off" id="txtEmail" name="Email" type="email" class="form-control" placeholder="@Resources.General.Email" required />
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="row">
                            <div class="col">
                                <label>@Resources.General.Country</label>
                                <select id="selectCountry" name="CountryId" required class="form-control">
                                    @foreach (Country country in Model.Countries)
                    {
        <option value="@country.Id">@country.Name</option>}
                                </select>
                            </div>

                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col">
                                <label>@Resources.General.Industry</label>
                                <select id="selectIndustry" name="IndustryId" required class="form-control">
                                    @foreach (Industry industry in Model.Industries)
                                    {
                        <option value="@industry.Id">@industry.Name</option>}
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col">
                                <label>@Resources.General.NumberOfEmployees</label>
                                <select id="employees" name="NumberOFEmployees" required class="form-control">
                                    @foreach (KeyValuePair<int, string> item in (new HumanResourceHelth.Model.NumberOfEmployees()).Employees)
                                    {
                        <option value="@item.Key">@item.Value</option>}
                                </select>
                            </div>
                        </div>
                    </div>



                    <div class="form-group">
                        <div class="row">
                            <div class="col">
                                <label>@Resources.General.ContactInformation</label>
                                <input id="txtInformation" name="ContactInformation" type="text" maxlength="255" class="form-control" placeholder="@Resources.General.ContactInformation" />


                            </div>
                            <div class="col">
                                <label>@Resources.General.ContactPerson</label>
                                <input autocomplete="off" name="ContactPerson" id="txtPerson" type="text" class="form-control" placeholder="@Resources.General.ContactPerson" />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <label>@Resources.General.Description</label>
                            <textarea id="txtDescription" name="Description" class="form-control" placeholder="@Resources.General.Description ...."></textarea>
                        </div>
                    </div>

                    <div class="padding-bottom-22px">
                        <a href="javascript:void(0)" id="btnSend" class="btn btn-sm background-main-color text-white padding-lr-15px border-0 opacity-8">@Resources.General.Send</a>
                    </div>


                </form>
            </div>
        </div>
    </div>
</section>

<script>
    $("#btnSend").on("click", function () {
            var form = $('#frmRequest');

            $.ajax({
                url: "@Url.Action("TechRequest", "Plans")",
                data: form.serialize(),
                type: "POST",

                success: function () {
                    toastr.success("@Resources.General.RequestSent")
            },
            error: function (err,status) {

                toastr.error("error")
            }
        });
    });
</script>