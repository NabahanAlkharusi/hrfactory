<section class="padding-tb-20px background-white">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 wow fadeInUp" style="visibility: visible; animation-name: fadeInUp;">
                <p>
                    Congratulations, Now you are subscribed in <b>@Model.Plan.Name</b> Plan.
                </p>
                <p>
                    Your subscribtion will be end at <b>@(Model.UserPlan.EndDate.ToString("dd MMM yyyy"))</b>.
                </p>
            </div>
        </div>
    </div>
</section>
<div class="container">
    <div class="row">
        @if (Model.Plan.Id == (int)HumanResourceHelth.Model.SubscriptionPlan.ManualBuilder)
        {
            <div class="col-4"></div>
            <div class="col-2">
                <a href="@Url.Action("Index", "Builder")" class="btn btn-sm background-main-color text-white padding-lr-15px border-0 opacity-8">
                    Build Your Document
                </a>
            </div>
            <div class="col-2">
                <a href="@Url.Action("ManualBuilder", "Plans")" class="btn btn-sm background-main-color text-white padding-lr-15px border-0 opacity-8">
                    Plan Details
                </a>
            </div>
        }
        @if (Model.Plan.Id == (int)HumanResourceHelth.Model.SubscriptionPlan.Startup)
        {
            <div class="col-3"></div>
            <div class="col-2">
                <a href="@Url.Action("Index", "Plans", new { plan = "StartUp"})" class="btn btn-sm background-main-color text-white padding-lr-15px border-0 opacity-8">
                    Documents
                </a>
            </div>
            <div class="col-2">
                <a href="@Url.Action("Intro", "Survey")" class="btn btn-sm background-main-color text-white padding-lr-15px border-0 opacity-8">
                    HR Heallth Check
                </a>
            </div>
            <div class="col-2">
                <a href="@Url.Action("StartUp", "Plans")" class="btn btn-sm background-main-color text-white padding-lr-15px border-0 opacity-8">
                    Plan Details
                </a>
            </div>
        }
    </div>
</div>
